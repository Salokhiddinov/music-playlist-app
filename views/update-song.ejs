<!-- include header -->
<%- include('include/_header.ejs')%>
<!-- /include header -->

<h2>Update User</h2>
<a href="/">&lt&lt All Users</a>


<form method="POST" id="update_song">
  <h2>Update Song</h2>
  <div class="form-item">
    <label for="name">Artist</label>
    <input type="hidden" name="id" value="" />
    <input type="text" name="name" placeholder="The Weeknd" />
  </div>
  <div class="form-item">
      <label for="title">Title</label>
      <input type="text" name="email" placeholder="Starboy" />
    </div>
    <div class="form-item">
      <label for="url">Link to Photo</label>
      <input type="url" name="email" placeholder="URL from Google" />
      <br><span>Here you can upload a photo of an artist by providing a URL to an image.</span>
    </div>
    <div class="form-item">
      <label for="email">Year</label>
      <input type="number" name="date" placeholder="2016" />
    </div>
  </form>



<!-- include footer -->
<%- include('include/_footer.ejs')%>
<!-- /include footer -->



